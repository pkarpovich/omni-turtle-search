{
  "manifest_version": 3,
  "name": "Omni Turtle Search",
  "description": "Search the web with the power of the turtle!",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://kagi.com/search?q=*"],
      "js": ["scripts/content.js", "scripts/SearchItemComponent.js"]
    }
  ],
  "permissions": ["activeTab"]
}